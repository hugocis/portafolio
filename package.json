{
  "name": "portafolios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:dev": "docker compose up --build",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "docker:server": "docker compose -f docker-compose.yml -f docker-compose.server.yml up --build -d",
    "docker:stop": "docker compose down",
    "docker:clean": "docker compose down -v",
    "docker:logs": "docker compose logs -f app",
    "docker:db": "docker compose exec db psql -U postgres -d portafolios"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
